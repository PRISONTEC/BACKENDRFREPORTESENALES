                continuar = false;
                break;

            case Sara.F_CONFIRMAR_REPORTE:
                tocarMensaje(canal, '/ivr/confirmarReporte', -1, -1, false);
                canal.estadoAplicacion = Sara.CONFIRMANDO_REPORTE;
                continuar = false;
                break;

